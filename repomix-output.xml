<file path="themes/PaperMod/assets/css/extended/mobile-first.css">
/* ========================================
   MOBILE-FIRST OPTIMIZATIONS
   Data: 28 de Outubro de 2025
   Metodologia: CSS ascendente (min-width)
   ======================================== */

/* ===== 1. DESIGN TOKENS ===== */
:root {
    /* Spacing System (8px baseline grid) */
    --spacing-xs: 4px;
    --spacing-sm: 8px;
    --spacing-md: 16px;
    --spacing-lg: 24px;
    --spacing-xl: 32px;
    --spacing-2xl: 48px;
    
    /* Legacy spacing (mantido por compatibilidade) */
    --gap: 14px;
    --content-gap: 16px;
    --nav-width: 100%;
    --main-width: 100%;
    
    /* Layout Heights */
    --header-height: 56px;
    --header-height-desktop: 72px;
    --footer-height: 56px;
    
    /* Menu Mobile Dimensions */
    --menu-width-mobile: clamp(304px, 88vw, 344px);
    --menu-padding-top: 64px;
    --menu-padding-horizontal: 24px;
    --menu-item-height: 56px;
    --menu-item-spacing: 20px;
    
    /* Touch Targets (WCAG 2.2 + Material Design) */
    --touch-target-min: 48px;
    --touch-target-spacing: 16px;
    
    /* Hamburger Button */
    --hamburger-size: 48px;
    --hamburger-line-width: 22px;
    --hamburger-line-height: 2.5px;
    --hamburger-line-gap: 6px;
    
    /* Border Radius */
    --radius-sm: 4px;
    --radius-md: 8px;
    --radius-lg: 12px;
    
    /* Typography */
    --font-size-base: 16px;
    --font-size-sm: 14px;
    --font-size-menu: 16px;
    --font-size-menu-desktop: 18px;
    --font-size-lg: 18px;
    --font-size-xl: 24px;
    --font-size-2xl: 32px;
    
    /* Shadows */
    --shadow-sm: 0 1px 2px rgba(0, 0, 0, 0.05);
    --shadow-md: 0 4px 6px rgba(0, 0, 0, 0.1);
    --shadow-lg: 0 10px 15px rgba(0, 0, 0, 0.15);
    
    /* Transitions */
    --transition-fast: 0.15s cubic-bezier(0.4, 0, 0.2, 1);
    --transition-base: 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    --transition-slow: 0.5s cubic-bezier(0.4, 0, 0.2, 1);
    
    /* Overlay */
    --overlay-opacity: 0.45;
}

/* ===== 2. HEADER & NAVIGATION (MOBILE BASE) ===== */

/* Header com glassmorphism */
.header {
    backdrop-filter: blur(10px);
    background: rgba(232, 237, 244, 0.98);
    border-bottom: 1px solid rgba(15, 20, 25, 0.1);
    box-shadow: var(--shadow-sm);
    transition: all var(--transition-base);
    position: sticky;
    top: 0;
    z-index: 100;
}

.dark .header {
    background: rgba(15, 20, 25, 0.98);
    border-bottom: 1px solid rgba(232, 237, 244, 0.1);
    box-shadow: var(--shadow-md);
}

/* Nav layout mobile */
.nav {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 var(--gap);
    min-height: var(--header-height);
}

/* Logo */
.logo {
    display: flex;
    align-items: center;
}

.logo a {
    font-weight: 600;
    font-size: var(--font-size-lg);
    transition: transform var(--transition-fast);
}

.logo a:hover {
    transform: translateY(-1px);
}

.logo-switches {
    display: flex;
    align-items: center;
    gap: 0.5rem;
}

/* Theme toggle */
#theme-toggle {
    border-radius: var(--radius-md);
    padding: 0.5rem;
    transition: all var(--transition-base);
    display: flex;
    align-items: center;
    justify-content: center;
    min-width: var(--touch-target-min);
    min-height: var(--touch-target-min);
}

#theme-toggle:hover {
    background: rgba(15, 20, 25, 0.08);
    transform: rotate(15deg) scale(1.05);
}

.dark #theme-toggle:hover {
    background: rgba(232, 237, 244, 0.08);
}

/* ===== 3. MENU MOBILE (BASE) ===== */

/* No-JS Fallback: details/summary (Progressive Enhancement) */
.menu-wrapper {
    position: relative;
}

/* Static menu button (always in HTML, shown only with JS + mobile viewport) */
.mobile-menu-btn-static {
    display: none; /* Hidden by default (no JS or desktop) */
    flex-direction: column;
    justify-content: center;
    align-items: center;
    gap: var(--hamburger-line-gap);
    width: var(--hamburger-size);
    height: var(--hamburger-size);
    margin-right: var(--touch-target-spacing);
    background: rgba(15, 20, 25, 0.05);
    border: 1px solid rgba(15, 20, 25, 0.15);
    border-radius: var(--radius-md);
    cursor: pointer;
    z-index: 1001;
    transition: all var(--transition-base);
    /* Increased touch area for better UX */
    padding: 0;
    position: relative;
}

/* Show static button when JS is available */
.js-enabled .mobile-menu-btn-static {
    display: flex;
}

.dark .mobile-menu-btn-static {
    background: rgba(232, 237, 244, 0.08);
    border-color: rgba(232, 237, 244, 0.2);
}

.mobile-menu-btn-static:hover {
    background: rgba(15, 20, 25, 0.1);
    transform: scale(1.02);
}

.dark .mobile-menu-btn-static:hover {
    background: rgba(232, 237, 244, 0.12);
}

.mobile-menu-btn-static span {
    display: block;
    width: var(--hamburger-line-width);
    height: var(--hamburger-line-height);
    background: var(--primary);
    border-radius: calc(var(--hamburger-line-height) / 2);
    transition: all var(--transition-base);
}

/* Animação X quando aberto - ajustada para linhas mais finas */
.mobile-menu-btn-static.active span:nth-child(1) {
    transform: rotate(45deg) translate(6.5px, 6.5px);
}

.mobile-menu-btn-static.active span:nth-child(2) {
    opacity: 0;
}

.mobile-menu-btn-static.active span:nth-child(3) {
    transform: rotate(-45deg) translate(6.5px, -6.5px);
}

/* Static overlay - improved UX with opacity transition */
.menu-overlay-static {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    background: rgba(0, 0, 0, var(--overlay-opacity));
    z-index: 999;
    cursor: pointer;
    opacity: 0;
    transition: opacity var(--transition-base);
}

.menu-overlay-static.show {
    display: block;
    opacity: 1;
}

/* Respeitar preferência de animações reduzidas */
@media (prefers-reduced-motion: reduce) {
    .menu-overlay-static {
        transition: none;
    }
}

/* Hide fallback button when JS is available */
.js-enabled .mobile-menu-btn-fallback {
    display: none !important;
}

/* Prevent details from affecting layout when JS is available */
.js-enabled .menu-wrapper {
    pointer-events: none; /* Disable details interaction */
}

.js-enabled .menu-wrapper #menu {
    pointer-events: auto; /* Re-enable menu interaction */
}

/* === RESPONSIVENESS: Hide mobile button on desktop === */
@media screen and (min-width: 1024px) {
    .mobile-menu-btn-static,
    .mobile-menu-btn-fallback,
    .menu-overlay-static {
        display: none !important;
    }
}

/* Fallback summary button (styled like hamburger) */
.mobile-menu-btn-fallback {
    display: flex;

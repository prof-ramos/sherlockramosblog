    opacity: 0;
}

.mobile-menu-btn.active span:nth-child(3) {
    transform: rotate(-45deg) translate(6.5px, -6.5px);
}

/* Menu mobile - sidebar deslizante (Material Design compliant) */
#menu {
    display: flex;
    position: fixed;
    top: 0;
    right: -100%;
    width: var(--menu-width-mobile);
    height: 100vh;
    background: var(--theme);
    padding: var(--menu-padding-top) 0 var(--spacing-lg);
    flex-direction: column;
    gap: var(--spacing-xs);
    align-items: stretch;
    transition: right var(--transition-slow);
    box-shadow: -4px 0 20px rgba(0, 0, 0, 0.3);
    z-index: 1000;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

/* Respeitar preferência de animações reduzidas */
@media (prefers-reduced-motion: reduce) {
    #menu {
        transition: none;
    }
}

.dark #menu {
    box-shadow: -4px 0 20px rgba(0, 0, 0, 0.6);
}

#menu.show-menu {
    right: 0;
}

/* Hide fallback menu when JS is enabled */
.js-enabled #menu-fallback {
    display: none !important;
}

/* No-JS Fallback: Show fallback menu when details is open */
.menu-wrapper[open] #menu-fallback {
    display: flex !important;
    right: 0;
}

.menu-wrapper[open] #menu-fallback li {
    opacity: 1;
    transform: translateX(0);
}

/* Style fallback menu same as main menu */
#menu-fallback {
    display: flex;
    position: fixed;
    top: 0;
    right: -100%;
    width: min(280px, 85vw);
    height: 100vh;
    background: var(--theme);
    padding: 80px 0 20px;
    flex-direction: column;
    gap: 0;
    list-style: none;
    margin: 0;
    box-shadow: -4px 0 20px rgba(0, 0, 0, 0.3);
    z-index: 1000;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    transition: right var(--transition-base);
}

.dark #menu-fallback {
    box-shadow: -4px 0 20px rgba(0, 0, 0, 0.6);
}

#menu-fallback li {
    width: 100%;
}

#menu-fallback li a {
    display: block;
    padding: 1rem 2rem;
    font-size: var(--font-size-lg);
    border-radius: 0;
    border-left: 3px solid transparent;
    transition: all var(--transition-base);
    min-height: var(--touch-target-min);
    display: flex;
    align-items: center;
}

#menu-fallback li a:hover,
#menu-fallback li a:active {
    background: rgba(15, 20, 25, 0.08);
    border-left-color: var(--primary);
    transform: translateX(4px);
}

.dark #menu-fallback li a:hover,
.dark #menu-fallback li a:active {
    background: rgba(232, 237, 244, 0.08);
}

/* Itens do menu */
#menu li {
    width: 100%;
    opacity: 0;
    transform: translateX(50px);
    transition: all var(--transition-base);
}

#menu.show-menu li {
    opacity: 1;
    transform: translateX(0);
}

/* Animação escalonada */
#menu.show-menu li:nth-child(1) { transition-delay: 0.1s; }
#menu.show-menu li:nth-child(2) { transition-delay: 0.15s; }
#menu.show-menu li:nth-child(3) { transition-delay: 0.2s; }
#menu.show-menu li:nth-child(4) { transition-delay: 0.25s; }
#menu.show-menu li:nth-child(5) { transition-delay: 0.3s; }

/* Links do menu - otimizado para touch targets (Material Design) */
#menu li a {
    display: flex;
    align-items: center;
    padding: var(--spacing-md) var(--menu-padding-horizontal);
    min-height: var(--menu-item-height);
    font-size: var(--font-size-menu);
    border-radius: 0;
    border-left: 4px solid transparent;
    transition: all var(--transition-base);
    position: relative;
}

#menu li a:hover,
#menu li a:active {
    background: rgba(15, 20, 25, 0.08);
    border-left-color: var(--primary);
    transform: translateX(6px);
}

.dark #menu li a:hover,
.dark #menu li a:active {
    background: rgba(232, 237, 244, 0.08);
}

/* Active page indicator - melhor contraste visual */
#menu li a span.active {
    color: var(--primary);
    font-weight: 600;
    position: relative;
}

#menu li a span.active::before {
    content: '';
    position: absolute;
    left: calc(-1 * var(--menu-padding-horizontal));
    top: 50%;
    transform: translateY(-50%);
    width: 4px;
    height: 100%;
    background: var(--primary);
    border-radius: 0 2px 2px 0;
}

/* Overlay escuro - usa token de opacity */
.menu-overlay {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0, 0, 0, var(--overlay-opacity));
    z-index: 999;
    backdrop-filter: blur(2px);
    opacity: 0;
    transition: opacity var(--transition-base);
}

.menu-overlay.show {
    display: block;
    opacity: 1;
}

/* Respeitar preferência de animações reduzidas */
@media (prefers-reduced-motion: reduce) {
    .menu-overlay {
        transition: none;
    }
}

/* Previne scroll do body quando menu aberto */
body.menu-open {
    overflow: hidden;
}

/* ===== 4. ACESSIBILIDADE ===== */

/* Focus states */
#menu li a:focus-visible,
#theme-toggle:focus-visible,
.mobile-menu-btn:focus-visible {
    outline: 2px solid var(--primary);
    outline-offset: 2px;
}

/* Prefers reduced motion */
@media (prefers-reduced-motion: reduce) {
    *,
    *::before,
    *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
    }
    
    .mobile-menu-btn span,
    #menu li {
        transition: none !important;
    }
}

/* ===== 5. BREAKPOINT TABLET (640px+) ===== */
@media (min-width: 640px) {
    :root {
        --gap: 18px;
        --content-gap: 18px;
        --header-height: 60px;
        --footer-height: 60px;
    }
    
    /* Menu mais largo */
    #menu {
        width: 320px;
    }
    
    /* Links com mais padding */
    #menu li a {
        padding: 1.25rem 2rem;
    }
}

/* ===== 6. BREAKPOINT DESKTOP (1024px+) ===== */
@media (min-width: 1024px) {
    :root {
        --gap: 24px;
        --content-gap: 20px;
        --nav-width: 1024px;
        --main-width: 720px;
        --header-height: var(--header-height-desktop);
    }
    
    /* Esconde botão hamburger e fallback */
    .mobile-menu-btn,
    .mobile-menu-btn-fallback {
        display: none !important;
    }
    
    /* Menu wrapper não afeta layout em desktop */
    .menu-wrapper {
        display: contents; /* Faz o wrapper "desaparecer" */
    }
    
    /* Header com altura aumentada */
    .header {
        min-height: var(--header-height-desktop);
    }
    
    .nav {
        padding: var(--spacing-lg) var(--gap);
        min-height: var(--header-height-desktop);
    }
    
    /* Menu horizontal - espaçamento otimizado */
    #menu {
        display: flex !important;
        position: static;
        flex-direction: row;
        width: auto;
        height: auto;
        background: transparent;
        padding: 0;
        margin: 0 var(--spacing-lg);
        gap: var(--spacing-lg); /* 24px conforme Material Design */
        box-shadow: none;
        align-items: center;
        overflow: visible;
        transition: none;
    }
    
    #menu li {
        width: auto;
        opacity: 1;
        transform: none;
        transition: none;
    }
    
    #menu li a {
        padding: var(--spacing-md) var(--spacing-lg);
        border-radius: var(--radius-md);
        border-left: none;
        font-size: var(--font-size-menu-desktop);
        min-height: auto;
        font-weight: 500;
    }
    
    #menu li a:hover {
        background: rgba(15, 20, 25, 0.08);
        transform: translateY(-2px);
    }
    
    .dark #menu li a:hover {
        background: rgba(232, 237, 244, 0.08);
    }
    
    /* Active indicator diferente - underline em vez de borda */
    #menu li a span.active {
        position: relative;
    }
    
    #menu li a span.active::before {
        display: none; /* Remove a barra lateral em desktop */
    }
    
    #menu li a span.active::after {
        content: '';
        position: absolute;
        bottom: -6px;
        left: 0;
        width: 100%;
        height: 3px;
        background: var(--primary);
        border-radius: 2px 2px 0 0;
    }
    
    /* Overlay nunca aparece */
    .menu-overlay {
        display: none !important;
    }
    
    /* Nav mais largo */
    .nav {
        max-width: calc(var(--nav-width) + var(--gap) * 2);
        margin-inline-start: auto;
        margin-inline-end: auto;
    }
}

/* ===== 7. BREAKPOINT WIDE (1440px+) ===== */
@media (min-width: 1440px) {
    :root {
        --nav-width: 1200px;
        --main-width: 800px;
    }
    
    #menu li a {
        padding: 0.5rem 1.25rem;
        font-size: 1rem;
    }
}

/* ===== 8. PRINT STYLES ===== */
@media print {
    .header,
    .mobile-menu-btn,
    #menu,
    #theme-toggle {
        display: none !important;
    }
}

        </div>
        {{- $currentPage := . }}
        {{- /* Mobile menu button (static in HTML, shown/hidden by CSS) */ -}}
        <button class="mobile-menu-btn-static" aria-label="Menu" aria-expanded="false">
            <span></span>
            <span></span>
            <span></span>
        </button>
        {{- /* No-JS Fallback: <details> with duplicate menu for when JS is disabled */ -}}
